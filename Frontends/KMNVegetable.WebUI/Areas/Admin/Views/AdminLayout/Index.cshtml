
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">

<head>

    @await Component.InvokeAsync("_AdminHeadComponentPartial")

</head>
<body>
    
    <div class="container-scroller">

        @await Component.InvokeAsync("_AdminLayoutNavbarComponentPartial")

        @await Component.InvokeAsync("_AdminLayoutTaskBarComponentPartial")
        
        <div class="container-fluid page-body-wrapper">

            @await Component.InvokeAsync("_AdminLayoutSideBarSkinsComponentPartial")

            @await Component.InvokeAsync("_AdminLayoutSettingsPanelComponentPartial")

            @await Component.InvokeAsync("_AdminLayoutSideBarComponentPartial")

            <div class="main-panel">
                <div class="content-wrapper">

                    <div class="row">

                        @RenderBody()

                    </div>

                </div>

                @await Component.InvokeAsync("_AdminLayoutFooterComponentPartial")

            </div>
        </div>
    </div>

    @await Component.InvokeAsync("_AdminLayoutScriptComponentPartial")

</body>

</html>
